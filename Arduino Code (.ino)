//Arduino Uno Code  8.11.2014


byte buttons[] = {16, 15, 14, 17, 18, 19}; 
#define NUMBUTTONS sizeof(buttons)
#include <Stepper.h>
#include <Servo.h> 

 const int stepsPerRevolution = 50;
// initialize the stepper library on pins 8 through 11:
Stepper myStepper(stepsPerRevolution, 8,9,10,11);   

 
Servo myservo;  // create servo object to control a servo 
                // a maximum of eight servo objects can be created 
 
int pos = 0, d=0;    // variable to store the servo position 
  // change this to fit the number of steps per revolution
                                     // for your motor



// the setup routine runs once when you press reset:
void setup() {
   myStepper.setSpeed(60);
  // initialize serial communication at 9600 bits per second:
  Serial.begin(9600);
  // make the pushbutton's pin an input:
  pinMode(14, INPUT);
  pinMode(15, INPUT);
  pinMode(16, INPUT);
  pinMode(17, INPUT);
  pinMode(18, INPUT);
  pinMode(19, INPUT);
   pinMode(12, INPUT);
   //  pinMode(0, INPUT);
    myservo.attach(9); 
     pinMode(2, OUTPUT);     
  pinMode(3, OUTPUT);  
    
}

// the loop routine runs over and over again forever:
void loop() {

 
  // print out the state of the button:
  Serial.println(analogRead(14));
    delay(1); 
   Serial.println(analogRead(15));
     delay(1); 
    Serial.println(analogRead(16));
      delay(1); 
     Serial.println(analogRead(17));
       delay(1); 
      Serial.println(analogRead(18));
        delay(1); 
       Serial.println(analogRead(19));
         delay(1); 
 Serial.println(digitalRead(12));
         delay(1); 
          Serial.println(digitalRead(0));
         delay(1); 
         Serial.println("    ");
delay(1000);

 if(digitalRead(12)==0)
{   Serial.println("clockwise");
  myStepper.step(stepsPerRevolution);
  delay(500);
}

      

      // 6 pressed, 1 case------------------------------------------------------------------------------------------
      
          if(analogRead(14)<1000 && analogRead(18)<1000 && analogRead(17)<1000 && analogRead(16)<1000 && analogRead(15)<1000 && analogRead(19)<1000 )
{ myservo.write(pos+d);  
 digitalWrite(2, HIGH);   // turn the LED on (HIGH is the voltage level)
  digitalWrite(3, LOW);    // turn the LED off by making the voltage LOW
  delay(500);  
   digitalWrite(2, LOW);   // turn the LED on (HIGH is the voltage level)
  digitalWrite(3, HIGH);    // turn the LED off by making the voltage LOW
  delay(500); // tell servo to go to position in variable 'pos' 
     Serial.println("smash!123456");}
     
       
   // 5 pressed, 6 cases-------------------------------------------------------------------------------------------------------------
  
